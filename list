n = int(input("Enter the number of colors in a list: "))
clr_list1 = []
clr_list2 = []
clr_list3 = []

print("Enter colors in clr_list1:")
for i in range(n):
    list1 = input()
    clr_list1.append(list1)

print("Enter colors in clr_list2:")
for i in range(n):
    list2 = input()
    clr_list2.append(list2)

for color in clr_list1:
    if color not in clr_list2:
        clr_list3.append(color)

print(f"Colors in clr_list1 that are not in clr_list2: {clr_list3}")
